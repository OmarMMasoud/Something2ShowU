# Enable URL rewriting
RewriteEngine On

# Custom rewrite rules for image paths
RewriteRule ^imgs/(.*)$ /wp-content/themes/something2showu/imgs/$1 [L]

# Handle React routing
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ /wp-content/themes/something2showu/index.html [L]